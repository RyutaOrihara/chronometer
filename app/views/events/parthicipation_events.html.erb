<h1>参加予定のイベント</h1>
<div class="container">
  <% unless @parthicipation_events.present? %>
  <br>
    <p>参加予定のイベントはありません。</p>
  <% else %>
  <div class="row">
    <% @parthicipation_events.each do |event| %>
      <div class="col-xs-12 col-sm-6 col-md-6 event-panel-primary">
        <h2><%= event.title %></h2>
        <h3><%= event.user.username %></h3>
        <%= link_to "詳細確認",event_path(event.id),class: 'btn btn-default' %>
      </div>
    <% end %>
  </div>
  <% end %>
  <div class="row pagenate">
    <%= paginate(@events) %>
  </div>
  <br>
    <%= link_to '戻る', events_path,class: "btn btn-outline-secondary" %>
</div>
